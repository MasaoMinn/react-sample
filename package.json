{
  "publisher": "tomjs-test",
  "name": "template-react",
  "type": "commonjs",
  "version": "0.0.0",
  "private": true,
  "description": "vite + react",
  "main": "dist/extension/index.js",
  "engines": {
    "node": ">=18",
    "vscode": "^1.75.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World: Show"
      }
    ],
    "views": {
      "hello-world-sidebar": [
        {
          "id": "helloWorldView",
          "name": "Hello World",
          "icon": "$(rocket)",
          "contextualTitle": "Hello World Extension"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hello-world-sidebar",
          "title": "Hello World",
          "icon": "$(rocket)"
        }
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tomjs/tsconfig": "^3.2.0",
    "@tomjs/vite-plugin-vscode": "^7.1.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.93.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}